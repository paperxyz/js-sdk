{
  "name": "js-sdk-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --continue --parallel",
    "dev-ews": "turbo run dev-ews --no-cache --continue --parallel",
    "dev-checkout": "turbo run dev --no-cache --continue --parallel",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build && changeset publish",
    "test": "turbo run test",
    "prepare": "ts-node ./scripts/postInstall"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.1",
    "@manypkg/cli": "^0.20.0",
    "eslint-config-paperxyz": "*",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "turbo": "latest"
  },
  "packageManager": "yarn@1.22.19"
}
