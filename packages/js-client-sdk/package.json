    "build": "tsup src/index.ts --env.NODE_ENV production",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@paperxyz/tsconfig": "*",
    "@types/node": "^18.7.11",
    "eslint-config-paperxyz": "*",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@paperxyz/sdk-common-utilities": "*",
    "ethers": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
